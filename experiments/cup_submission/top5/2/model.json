{
    "optimizer": {
        "Learning Rate": 5e-05,
        "Momentum": 0.6
    },
    "batch_size": 12,
    "epochs": 20000,
    "loss": "mean_squared_error",
    "metrics": [
        "root_mean_squared_error",
        "mean_euclidean_error"
    ],
    "callback": {
        "Patience": 50,
        "Start From Epoch": 5000,
        "Delta": 0.0001,
        "Monitor": "mean_squared_error",
        "Restore Best Weights": true
    },
    "regularizer": {
        "l1": 0.0
    },
    "layers": [
        {
            "input_size": 10,
            "output_size": 128,
            "weights_initializer": "GlorotUniform",
            "bias_initializer": "Zeros",
            "activation": "ReLu"
        },
        {
            "input_size": 128,
            "output_size": 64,
            "weights_initializer": "GlorotUniform",
            "bias_initializer": "Zeros",
            "activation": "Tanh"
        },
        {
            "input_size": 64,
            "output_size": 3,
            "weights_initializer": "GlorotUniform",
            "bias_initializer": "Zeros",
            "activation": "Identity"
        }
    ]
}